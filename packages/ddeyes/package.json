{
  "name": "ddeyes",
  "version": "0.2.14",
  "main": "dist/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "autod": "autod",
    "clean": "rm -rf ./dist",
    "fixInspectCall": "sed -i \"s/(0, _eyes.inspector)/_eyes.inspector/g\" dist/index.js",
    "build": "coffee -o ./dist/module.js ./src; babel -o dist/index.js --presets @babel/preset-env --plugins @babel/plugin-transform-runtime dist/module.js; npm run fixInspectCall",
    "rebuild": "npm run clean; npm run build",
    "test": "./test/index.coffee",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@babel/runtime": "~7.1.2",
    "eyes": "~0.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "autod": "^3.0.1",
    "coffeescript": "^2.3.2",
    "shelljs": "^0.8.2"
  }
}
